script/source = "extends CharacterBody2D

var time : int = 120
var tick : bool
var traveltimestart : float
var traveltime : float
var origin: int

func _ready() -> void:
	origin = position.y

func _physics_process(delta):
	move_and_slide()
	if velocity.y < 0:
		traveltime -= velocity.y * -0.02
	if velocity.y > 0:
		traveltime -= velocity.y * 0.02
	
	
	if traveltime <= (traveltimestart * 0.75) and $AnimatedSprite2D.animation != (\"Wind\"):
		$AnimatedSprite2D.play(\"Wind\")
		
	if $AnimatedSprite2D.frame == 4:
		queue_free()
"

